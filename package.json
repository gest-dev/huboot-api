{
    "name": "huboot-api",
    "version": "0.4.0",
    "description": "huboot-api is builton top of Baileys-MD.",
    "main": "server.js",
    "scripts": {
        "start": "node src/server.js",
        "dev": "nodemon",
        "format:check": "prettier --check .",
        "format:write": "prettier --write .",
        "lint:check": "eslint .",
        "lint:fix": "eslint --fix .",
        "test": "mocha tests/*.test.js --exit",
        "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\""
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "prettier --write",
            "git add"
        ],
        "*.{html,css,less,ejs}": [
            "prettier --write",
            "git add"
        ]
    },
    "repository": "git@github.com:gezerramos/huboot-api",
    "author": "Gezer <gezer@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "@adiwajshing/keyed-db": "^0.2.4",
        "@whiskeysockets/baileys": "^6.7.13",
        "axios": "^1.7.9",
        "bcrypt": "^5.1.0",
        "bcryptjs": "^2.4.3",
        "connect-flash": "^0.1.1",
        "cookie-parser": "^1.4.7",
        "date-fns": "^4.1.0",
        "dotenv": "^16.4.5",
        "ejs": "^3.1.10",
        "express": "^4.21.1",
        "express-exception-handler": "^1.3.108",
        "express-rate-limit": "^7.4.0",
        "express-session": "^1.18.1",
        "joi": "^17.8.3",
        "joi-translation-pt-br": "^0.0.10",
        "jsonwebtoken": "^9.0.2 ",
        "jwt-decode": "^4.0.0",
        "link-preview-js": "^3.0.5",
        "mongodb": "^6.13.0",
        "mongoose": "^8.10.0",
        "multer": "^1.4.5-lts.1",
        "pino": "^9.3.2",
        "qrcode": "^1.5.4",
        "sharp": "^0.32.6",
        "uuid": "^11.0.3"
    },
    "devDependencies": {
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "husky": "^8.0.2",
        "lint-staged": "^13.0.4",
        "mocha": "^10.1.0",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.0",
        "supertest": "^6.3.1"
    }
}
